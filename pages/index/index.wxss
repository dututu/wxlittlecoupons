.container {
  width:100%; 
  margin:0 atuo;
}
/*首页搜索部分样式*/
.index_tit {
  text-align: center;
  margin:0 65rpx 20rpx;
  height: 60rpx;
  border-radius: 100rpx;
  background-color: #F8F8F8;
  font-family: PingFang-SC-Regular;
  font-size: 26rpx;
  color: #9B9B9B;
  line-height: 60rpx;
}
.index_tit_img {
  width: 30rpx;
  height: 30rpx;
  vertical-align:middle;
}
 /*首页轮播图的样式*/
 .index_banner {
   width: 100%;
   height: 256rpx;
 }
 .banner_box {
   width: 100%;
   height: 100%;
 }
 .slide-image {
   width: 100%;
   height: 100%;
 }
 /*首页导航的样式*/
 .index_nav {
   width: 100%;
   height: 360rpx;
   background-color: #fff;
 }
.index_nav_per {
  width: 20%;
  height: 180rpx;
  float: left;
  position: relative;
}
.index_nav_per image {
  width: 88rpx;
  height: 88rpx;
  position: absolute;
  top: 32rpx;
  left: 50%;
  transform: translateX(-50%);
}
.index_nav_per text {
  font-size: 28rpx;
  font-family: "苹方";
  position: absolute;
  bottom: 10rpx;
  left: 50%;
  transform: translateX(-50%);
}
.index_nav_per text.color {
  color: #666666;
}
.index_nav_per text.color0 {
  color: #ED4243;
}
.index_nav_per text.color1 {
  color: #FA8030;
}
.index_nav_per text.color2 {
  color: #FD5069;
}
.index_nav_per text.color3 {
  color: #EA5422;
}
.index_nav_per text.color4 {
  color: #7D514A;
}
.index_nav_per text.color5 {
  color: #6B4DFB;
}
.index_nav_per text.color6 {
  color: #E91B67;
}
.index_nav_per text.color7 {
  color: #21C2AC;
}
.index_nav_per text.color8 {
  color: #2B8AF1;
}
.index_nav_per text.color9 {
  color: #78CD39;
}


/*首页优惠券的样式*/
.index_detail {
  background-color: #EBEBEB;
  width:100%;
  height: 100%;
  padding:20rpx 30rpx; 
}
.index_poster {
  width: 690rpx;
  height: 80rpx;
  background: #FFFFFF;
  box-shadow: 1rpx 2rpx 7rpx 4rpx rgba(0,0,0,0.07);
  border-radius: 100rpx;
  display: flex;
  line-height: 80rpx;
}
.index_poster_tittle {
  width: 48rpx;
  height: 33rpx;
  padding-left: 16rpx;
  font-size: 24rpx;
  color: #000000;
}
.index_poster_type {
  width: 48rpx;
  height: 33rpx;
  font-family: PingFangSC-Semibold;
  padding-left: 5rpx;
  font-size: 24rpx;
  color: #EF3053;
}
.index_poster_content {
  width: 400rpx;
  height: 28rpx;
  padding-left: 10rpx;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #666666;
}
.index_poster_btn {
  position: absolute;
  width: 120rpx;
  height: 60rpx;
  margin: 10rpx 0;
  background: #EF3053;
  box-shadow: 0 4rpx 5rpx 0 rgba(239,48,83,0.30);
  border-radius: 26rpx;
  font-family: PingFangSC-Semibold;
  font-size: 24rpx;
  color: #FFFFFF;
  line-height: 60rpx;
  text-align: center;
  right: 40rpx;
}
